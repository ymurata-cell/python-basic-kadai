      "source": [
        "def calc_with_tax(price, tax_rate):\n",
        "    # 税込金額を計算（税率は％で受け取る）\n",
        "    result = price * (1 + tax_rate / 100)\n",
        "    return result\n",
        "\n",
        "# 商品金額 110円、消費税 10%\n",
        "total = calc_with_tax(110,10)\n",
        "print(f\"{total}円\")"
